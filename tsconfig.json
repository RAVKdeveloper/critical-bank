{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@libs/core": [
        "libs/core/src"
      ],
      "@libs/core/*": [
        "libs/core/src/*"
      ],
      "@libs/repository": [
        "libs/repository/src"
      ],
      "@libs/repository/*": [
        "libs/repository/src/*"
      ],
      "@libs/config": [
        "libs/config/src"
      ],
      "@libs/config/*": [
        "libs/config/src/*"
      ],
      "@libs/constants": [
        "libs/constants/src"
      ],
      "@libs/constants/*": [
        "libs/constants/src/*"
      ],
      "@libs/grpc-types": [
        "libs/grpc-types/src"
      ],
      "@libs/grpc-types/*": [
        "libs/grpc-types/src/*"
      ],
      "@libs/cache": [
        "libs/cache/src"
      ],
      "@libs/cache/*": [
        "libs/cache/src/*"
      ],
      "@lib/micro-clients": [
        "libs/micro-clients/src"
      ],
      "@lib/micro-clients/*": [
        "libs/micro-clients/src/*"
      ],
      "@lib/kafka-types": [
        "libs/kafka-types/src"
      ],
      "@lib/kafka-types/*": [
        "libs/kafka-types/src/*"
      ],
      "@lib/loki": [
        "libs/loki/src"
      ],
      "@lib/loki/*": [
        "libs/loki/src/*"
      ],
      "@lib/mongo-repo": [
        "libs/mongo-repo/src"
      ],
      "@lib/mongo-repo/*": [
        "libs/mongo-repo/src/*"
      ],
      "@lib/tg-bot": [
        "libs/tg-bot/src"
      ],
      "@lib/tg-bot/*": [
        "libs/tg-bot/src/*"
      ],
      "@lib/rate-limiter": [
        "libs/rate-limiter/src"
      ],
      "@lib/rate-limiter/*": [
        "libs/rate-limiter/src/*"
      ],
      "@lib/crypto": [
        "libs/crypto/src"
      ],
      "@lib/crypto/*": [
        "libs/crypto/src/*"
      ],
      "@lib/tokens": [
        "libs/tokens/src"
      ],
      "@lib/tokens/*": [
        "libs/tokens/src/*"
      ],
      "@lib/core-auth": [
        "libs/core-auth/src"
      ],
      "@lib/core-auth/*": [
        "libs/core-auth/src/*"
      ],
      "@lib/web3": [
        "libs/web3/src"
      ],
      "@lib/web3/*": [
        "libs/web3/src/*"
      ]
    }
  },
  "include": [
    "./apps",
    "./libs",
    "libs/core/src/types"
  ]
}