{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "./",
  "entryFile": "src/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json",
    "assets": [
      {
        "include": "proto/entities.proto",
        "outDir": "dist/apps/api-gateway/proto",
        "watchAssets": true
      },
      {
        "include": "proto/*.proto",
        "outDir": "dist/apps/api-gateway",
        "watchAssets": true
      }
    ],
    "watchAssets": true
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json",
        "assets": [
          {
            "include": "proto/entities.proto",
            "outDir": "dist/apps/auth/proto",
            "watchAssets": true
          },
          {
            "include": "proto/*.proto",
            "outDir": "dist/apps/auth",
            "watchAssets": true
          }
        ],
        "watchAssets": true
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "repository": {
      "type": "library",
      "root": "libs/repository",
      "entryFile": "index",
      "sourceRoot": "libs/repository/src",
      "compilerOptions": {
        "tsConfigPath": "libs/repository/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "constants": {
      "type": "library",
      "root": "libs/constants",
      "entryFile": "index",
      "sourceRoot": "libs/constants/src",
      "compilerOptions": {
        "tsConfigPath": "libs/constants/tsconfig.lib.json"
      }
    },
    "grpc-types": {
      "type": "library",
      "root": "libs/grpc-types",
      "entryFile": "index",
      "sourceRoot": "libs/grpc-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/grpc-types/tsconfig.lib.json"
      }
    },
    "cache": {
      "type": "library",
      "root": "libs/cache",
      "entryFile": "index",
      "sourceRoot": "libs/cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache/tsconfig.lib.json"
      }
    },
    "micro-clients": {
      "type": "library",
      "root": "libs/micro-clients",
      "entryFile": "index",
      "sourceRoot": "libs/micro-clients/src",
      "compilerOptions": {
        "tsConfigPath": "libs/micro-clients/tsconfig.lib.json"
      }
    },
    "kafka-types": {
      "type": "library",
      "root": "libs/kafka-types",
      "entryFile": "index",
      "sourceRoot": "libs/kafka-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka-types/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "loki": {
      "type": "library",
      "root": "libs/loki",
      "entryFile": "index",
      "sourceRoot": "libs/loki/src",
      "compilerOptions": {
        "tsConfigPath": "libs/loki/tsconfig.lib.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json",
        "assets": [
          "notifications/src/**/*.hbs"
        ],
        "watchAssets": true
      }
    },
    "mongo-repo": {
      "type": "library",
      "root": "libs/mongo-repo",
      "entryFile": "index",
      "sourceRoot": "libs/mongo-repo/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongo-repo/tsconfig.lib.json"
      }
    },
    "tg-bot": {
      "type": "library",
      "root": "libs/tg-bot",
      "entryFile": "index",
      "sourceRoot": "libs/tg-bot/src",
      "compilerOptions": {
        "tsConfigPath": "libs/tg-bot/tsconfig.lib.json"
      }
    },
    "rate-limiter": {
      "type": "library",
      "root": "libs/rate-limiter",
      "entryFile": "index",
      "sourceRoot": "libs/rate-limiter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rate-limiter/tsconfig.lib.json"
      }
    },
    "crypto": {
      "type": "library",
      "root": "libs/crypto",
      "entryFile": "index",
      "sourceRoot": "libs/crypto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/crypto/tsconfig.lib.json"
      }
    }
  }
}